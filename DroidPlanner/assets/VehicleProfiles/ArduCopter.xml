<?xml version="1.0" encoding="utf-8"?>
<Profile>
    <ParameterMetadataType
        type="ArduCopter2"/>

    <MissionViews>
        <MissionView
            id="@+id/altitudeView"
            visibility="visible"
            type="SeekBarWithText"
            min="0"
            max="120"
            inc="5"/>

        <MissionView
            id="@+id/waypointDelay"
            visibility="visible"
            type="SeekBarWithText"
            min="0"
            max="300"
            inc="5"/>

        <MissionView
            id="@+id/waypointAcceptanceRadius"
            visibility="gone"/>

        <MissionView
            id="@+id/optionLayout"
            visibility="gone"/>

        <MissionView
            id="@+id/waypointAngle"
            visibility="gone"/>

        <MissionView
            id="@+id/takeoffPitch"
            visibility="gone"/>
    </MissionViews>
</Profile>